            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=nat20-rs"
                ],
                "filter": {
                    "name": "nat20_rs",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
